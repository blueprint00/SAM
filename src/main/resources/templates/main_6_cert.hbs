                                                    <li><p id="competition_name">{{competition_name}}</p><br><p id="competition_score">{{competition_score}}</p><br><p id="competition_date">{{competition_date}}</p></li>
                                                   
                                                </ul>
                                                {{/competition}}
                                                <footer></footer>
                                            </div>
                                        </div>
        
        
        
                                    </div>
                                </div>
