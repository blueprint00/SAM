        .list-hover-slide li {
            position: relative;
            overflow: hidden;
        }
        
        .list-hover-slide a {
            display: block;
            position: relative;
            z-index: 1;
            transition: .35s ease color;
        }
        
        .list-hover-slide a:before {
            content: '';
            display: block;
            z-index: -1;
            position: absolute;
            left: -100%;
            top: 0;
            width: 100%;
            height: 100%;
            border-right: solid 5px #BE354D;
            background: #3C3735;
            transition: .35s ease left;
        }
        
        .list-hover-slide a.is-current:before,
        .list-hover-slide a:hover:before {
            left: 0;
        }
        
        #projectdiv {
            border: 3px solid white;
            border-radius: 15px;
            overflow: auto;
            height: 300px;
            box-shadow: 10px 10px #bcbdc8;
        }
        
        body>#projectdiv {
            transition: all 0.3s ease;
        }
        
        .grow {
