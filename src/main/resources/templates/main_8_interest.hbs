            letter-spacing: 3px;
            color: #152536;
            font-family: "Grotesque Black", sans-serif;
            font-size: 1rem;
            text-transform: uppercase;
            text-shadow: 0 0 0px #32577f;
        }
        
        article.card .infos .flag {
            position: absolute;
            top: 50%;
            right: 0;
            -webkit-transform: translateY(-50%);
            transform: translateY(-50%);
            width: 35px;
            height: 23px;
