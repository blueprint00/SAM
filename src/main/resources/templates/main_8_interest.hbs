        }
        
        a[href]::after {
            content: "";
            position: absolute;
            left: 0;
            left: 0;
            bottom: 0;
            background: #fff;
            width: 0;
            height: 1px;
            transition: 0.35s cubic-bezier(0.17, 0.67, 0.5, 1.03);
        }
        
        a[href]:hover::after {
            width: 100%;
            box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3);
        }
        
        .note {
            margin-top: 30px;
